(VAR X Y )
(STRATEGY INNERMOST)
(RULES 
        if(true) -> X
        if(false) -> Y
        fact(X) -> if(zero(X))
        add(0,X) -> X
        add(s(X),Y) -> s(add(X,Y))
        prod(0,X) -> 0
        prod(s(X),Y) -> add(Y,prod(X,Y))
        zero(0) -> true
        zero(s(X)) -> false
        p(s(X)) -> X
        
)
    