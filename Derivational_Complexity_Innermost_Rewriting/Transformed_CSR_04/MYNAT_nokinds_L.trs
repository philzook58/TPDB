(VAR M N X )
(STRATEGY INNERMOST)
(RULES 
        U11(tt) -> N
        U21(tt) -> s(plus(N,M))
        U41(tt) -> plus(x(N,M),N)
        and(tt) -> X
        U31(tt) -> 0
        isNat -> tt
